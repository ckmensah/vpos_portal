<p id="notice"><%= notice %></p>

<h1>Activity Sub Div Classes</h1>

<table>
  <thead>
    <tr>
      <th>Entity div code</th>
      <th>Class desc</th>
      <th>Comment</th>
      <th>Active status</th>
      <th>Del status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_sub_div_classes.each do |activity_sub_div_class| %>
      <tr>
        <td><%= activity_sub_div_class.entity_div_code %></td>
        <td><%= activity_sub_div_class.class_desc %></td>
        <td><%= activity_sub_div_class.comment %></td>
        <td><%= activity_sub_div_class.active_status %></td>
        <td><%= activity_sub_div_class.del_status %></td>
        <td><%= activity_sub_div_class.user_id %></td>
        <td><%= link_to 'Show', activity_sub_div_class %></td>
        <td><%= link_to 'Edit', edit_activity_sub_div_class_path(activity_sub_div_class) %></td>
        <td><%= link_to 'Destroy', activity_sub_div_class, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity Sub Div Class', new_activity_sub_div_class_path %>
