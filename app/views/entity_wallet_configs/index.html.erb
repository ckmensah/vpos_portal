<p id="notice"><%= notice %></p>

<h1>Entity Wallet Configs</h1>

<table>
  <thead>
    <tr>
      <th>Division code</th>
      <th>Service</th>
      <th>Secret key</th>
      <th>Client key</th>
      <th>Comment</th>
      <th>Active status</th>
      <th>Del status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entity_wallet_configs.each do |entity_wallet_config| %>
      <tr>
        <td><%= entity_wallet_config.division_code %></td>
        <td><%= entity_wallet_config.service_id %></td>
        <td><%= entity_wallet_config.secret_key %></td>
        <td><%= entity_wallet_config.client_key %></td>
        <td><%= entity_wallet_config.comment %></td>
        <td><%= entity_wallet_config.active_status %></td>
        <td><%= entity_wallet_config.del_status %></td>
        <td><%= entity_wallet_config.user_id %></td>
        <td><%= link_to 'Show', entity_wallet_config %></td>
        <td><%= link_to 'Edit', edit_entity_wallet_config_path(entity_wallet_config) %></td>
        <td><%= link_to 'Destroy', entity_wallet_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entity Wallet Config', new_entity_wallet_config_path %>
