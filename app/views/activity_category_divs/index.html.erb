<p id="notice"><%= notice %></p>

<h1>Activity Category Divs</h1>

<table>
  <thead>
    <tr>
      <th>Division code</th>
      <th>Activity category</th>
      <th>Activity div cat</th>
      <th>Category div desc</th>
      <th>Comment</th>
      <th>Active status</th>
      <th>Del status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_category_divs.each do |activity_category_div| %>
      <tr>
        <td><%= activity_category_div.division_code %></td>
        <td><%= activity_category_div.activity_category_id %></td>
        <td><%= activity_category_div.activity_div_cat_id %></td>
        <td><%= activity_category_div.category_div_desc %></td>
        <td><%= activity_category_div.comment %></td>
        <td><%= activity_category_div.active_status %></td>
        <td><%= activity_category_div.del_status %></td>
        <td><%= activity_category_div.user_id %></td>
        <td><%= link_to 'Show', activity_category_div %></td>
        <td><%= link_to 'Edit', edit_activity_category_div_path(activity_category_div) %></td>
        <td><%= link_to 'Destroy', activity_category_div, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity Category Div', new_activity_category_div_path %>
