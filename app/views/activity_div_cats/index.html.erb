<p id="notice"><%= notice %></p>

<h1>Activity Div Cats</h1>

<table>
  <thead>
    <tr>
      <th>Division code</th>
      <th>Div cat desc</th>
      <th>Comment</th>
      <th>Active status</th>
      <th>Del status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_div_cats.each do |activity_div_cat| %>
      <tr>
        <td><%= activity_div_cat.division_code %></td>
        <td><%= activity_div_cat.div_cat_desc %></td>
        <td><%= activity_div_cat.comment %></td>
        <td><%= activity_div_cat.active_status %></td>
        <td><%= activity_div_cat.del_status %></td>
        <td><%= activity_div_cat.user_id %></td>
        <td><%= link_to 'Show', activity_div_cat %></td>
        <td><%= link_to 'Edit', edit_activity_div_cat_path(activity_div_cat) %></td>
        <td><%= link_to 'Destroy', activity_div_cat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity Div Cat', new_activity_div_cat_path %>
