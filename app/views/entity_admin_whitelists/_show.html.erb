              </table>


            </div>

            <br/>

            <br/>

            <br/>
            <div class="w3-center">
              <%= link_to 'Back', entity_admin_whitelist_index_path(entity_code: params[:entity_code], code: params[:code], page: params[:page], count: params[:count]), class: 'w3-bar-item w3-btn w3-ripple w3-round-large w3-dark-gray', remote: true %>
