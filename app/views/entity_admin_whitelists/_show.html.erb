                  <% logger.info "SERVICE ACCOUNT DETAILS PASSED ===============================" %>
                  <% num = 1 %>
                  <% @display.each do |key, value| %>
                    <% if value["full_name"].present? && value["mobile_number"].present? %>
                      <% service = EntityDivision.where(active_status: true, assigned_code: @entity_admin_whitelist_params[:entity_division_code]).order(created_at: :desc).first %>
                      <tr>
                        <td><%= num %></td>
                        <td><%= service.division_name if service %> </td>
                        <td><%= value["full_name"] %></td>
                        <td><%= value["mobile_number"] %></td>
<!--                        <td><%#= value["activity_type_code"] %></td>-->

                        <% num = num + 1 %>
                      </tr>

                    <% end %>
                  <% end %>

                <% end %>

