
<%= simple_form_for(@external_services_param) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :entity_div_code %>
    <%= f.input :username %>
    <%= f.input :password %>
    <%= f.input :company_code %>
    <%= f.input :company_url %>
    <%= f.input :user_id %>
    <%= f.input :active_status %>
    <%= f.input :del_status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
