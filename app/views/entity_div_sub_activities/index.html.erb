<p id="notice"><%= notice %></p>

<h1>Entity Div Sub Activities</h1>

<table>
  <thead>
    <tr>
      <th>Sub activity code</th>
      <th>Div sub activity desc</th>
      <th>Entity div code</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entity_div_sub_activities.each do |entity_div_sub_activity| %>
      <tr>
        <td><%= entity_div_sub_activity.sub_activity_code %></td>
        <td><%= entity_div_sub_activity.div_sub_activity_desc %></td>
        <td><%= entity_div_sub_activity.entity_div_code %></td>
        <td><%= entity_div_sub_activity.user_id %></td>
        <td><%= entity_div_sub_activity.active_status %></td>
        <td><%= entity_div_sub_activity.del_status %></td>
        <td><%= link_to 'Show', entity_div_sub_activity %></td>
        <td><%= link_to 'Edit', edit_entity_div_sub_activity_path(entity_div_sub_activity) %></td>
        <td><%= link_to 'Destroy', entity_div_sub_activity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entity Div Sub Activity', new_entity_div_sub_activity_path %>
