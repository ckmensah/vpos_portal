


<div class="w3-container" >
  <div >
    <div class="w3-white">
      <p id="notes22"><%#= notice %></p>
      <div class="w3-container">

        <%= link_to 'NEW VIDEOS', inner_new_path(medium_type: "videos", code: params[:code], entity_code: params[:entity_code], count1: params[:count1],  count4: params[:count4], count: params[:count], page: params[:page]),
                    style: "width:120px; height: 30px;",
                    class: 'w3-button w3-purple-vpos w3-border w3-border-purple w3-tiny w3-round-large', remote:true %>
        <br><br>
        <div class="w3-responsive w3-border w3-border-purple pre-scrollable">
          <% @video_media.each do |video_media| %>
            <div class=" w3-responsive w3-container w3-half">
              <br>
              <div class="w3-center">
<!--                <br>-->
                <div style="display: block">
                  <% if video_media.active_status && video_media.del_status == false %>

                    <%= link_to 'EDIT', edit_entity_div_medium_path(video_media, medium_type: "videos", code: params[:code], entity_code: params[:entity_code], count1: params[:count1],  count4: params[:count4], count: params[:count], page: params[:page]),
                                style: "width:80px; height: 30px;",
                                class: 'w3-button w3-purple-vpos w3-border w3-border-purple w3-tiny w3-round-large', remote:true %>
                  <% else %>

                    <button class="w3-button w3-white w3-border w3-disabled w3-border-purple w3-tiny w3-round-large" style="width:100px; height: 30px; color: #800754 !important">EDIT</button>
                  <% end %>
                  <% if video_media.active_status %>
                    <%= link_to 'DISABLE', inner_destroy_path(video_media, medium_type: "videos", code: params[:code], entity_code: params[:entity_code], count1: params[:count1], count4: params[:count4], count: params[:count], page: params[:page]),
                                style: "width:100px; height: 30px; color: #800754 !important",
                                class: 'w3-button w3-white w3-border w3-border-purple w3-tiny w3-round-large', remote:true %>
                  <% else %>
                    <%= link_to 'ENABLE', inner_destroy_path(video_media, medium_type: "videos", code: params[:code], entity_code: params[:entity_code], count1: params[:count1], count4: params[:count4], count: params[:count], page: params[:page]),
                                style: "width:100px; height: 30px;",
                                class: 'w3-button w3-purple-vpos w3-border w3-border-purple w3-tiny w3-round-large', remote:true %>
                  <% end %>
                </div>
                <br>
                <div style="display: block">
                  <% if video_media.active_status %>
                    <%=
                      cl_video_tag("#{video_media.media_data}",
                                   :loop => true, :controls => true,
                                   :transformation => [
                                       :height=>250, :crop=>"pad",
                                       :quality=>70, :duration=>"60"],
                                   :fallback_content => "Your browser does not support HTML5 video tags." )
                    %>
                  <% else %>
                    <%=
                      cl_video_tag("#{video_media.media_data}", :controls => true, secure: true,
                                   :transformation => [{:height=>250, :crop=>"pad", :quality=>30, :duration=>"1"}],
                                   :fallback_content => "Your browser does not support HTML5 video tags." )
                    %>
                  <% end %>

                </div>
                <hr>
<!--                <video class="w3-hover-shadow w3-card-2 w3-border w3-round-xlarge" height=250 src="<%#= "https://res.cloudinary.com/appsnmob/#{video_media.media_data}" %>" type="video/*" controls="controls" loop="loop"></video>-->
<!--                <video class="w3-hover-shadow w3-card-2 w3-border w3-round-xlarge" height=250 controls="controls" >-->
<!--                  <source src="<%#= "https://res.cloudinary.com/appsnmob/#{video_media.media_data}" %>" type="video/*" />-->
<!--                </video>-->

<!--                <br>-->
              </div>
              <br>
            </div>
          <% end %>
        </div>
<!--        <br><br>-->
      </div>



    </div>



<!--    <br/>-->
    <div class="col-md-7 col-md-offset-5">
<!--      <br/>-->
<!--      <br/>-->
    </div>
    <!--</div>-->

  </div>
</div>




